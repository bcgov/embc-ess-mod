        width: 21.6cm;
        height: 27.9cm;
        padding: 0.75cm;
    }

    .with-watermark {
        background-image: url('data:image/svg+xml;base64,');
        background-repeat: repeat-y;
        background-position: 0 4cm;
    }

    .page-border {
        position: absolute;
        z-index: 1;
        border: 1px solid black;
        display: block;
        width: 20.2cm;
        height: 26cm;
        margin-left: -0.1cm;
    }

    .row {
        display: flex;
        margin-left: 0;
        margin-right: 0;
    }

    /* We are loosely emulating bootstrap here. */
    .col-4,
    .col-5,
    .col-6,
    .col-7,
    .col-12 {
        padding-left: 0;
        padding-right: 15px;
    }

    .col-4 {
        float: left;
        width: 33.33%;
    }

    .col-5 {
        float: left;
        width: 41.666667%;
    }

    .col-6 {
        float: left;
        width: 50%;
    }

    .col-7 {
        float: left;
        width: 58.333333%;
    }

    .col-12 {
        width: 100%;
    }

    .col-pushed {
        margin-left: 0.65cm;
        padding-right: calc(5px + 20px); /* whitespace + offset for next column's icon */
    }

    .push-right {
        text-align: right;
    }

    .no-padding {
        padding-right: 0;
    }

    .one-time-use {
        position: absolute;
        z-index: 5;
