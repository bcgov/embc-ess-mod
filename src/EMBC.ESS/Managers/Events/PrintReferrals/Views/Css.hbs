            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.meals:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.grocery:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.incidentals:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.clothing:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.housing:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.comments:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.notes:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.person:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.clipboard:before {
            background-image: url('data:image/svg+xml;base64,');
        }

        .info-block h3.with-icon.taxi:before {
            background-image: url('data:image/svg+xml;base64,');
        }

    .info-block p {
        margin: 0 0 10px;
    }

    .info-block label {
        font-weight: 600;
        padding-right: 5px;
    }

        .info-block label.blocked-label {
            display: inline-block;
            width: 1cm;
        }

    .sample-only {
        text-align: center;
        font-size: 50px;
        margin: 3cm 0;
    }

    /* This is where textboxes/inputs will render their outputs on the pdf */
    .input-output {
        padding-right: 3px;
        font-size: 12px;
        word-break: break-all;
    }

    /* These are the notes in the summary */
    .notes + .input-output {
        word-break: normal;
    }

    .comments-height {
        max-height: calc(12px * 5); /* 5 rows */
        width: calc(6.1px * 50); /* 50 cols */
        overflow-y: hidden;
        font-family: 'LiberationMono', monospace;
        font-size: 10px;
        line-height: normal;
        white-space: pre-wrap;
        word-break: break-all;
        margin-bottom: 0 !important;
    }

    .incidentals-height {
        max-height: calc(12px * 5); /* 5 rows */
        width: calc(6.1px * 50); /* 50 cols */
        overflow-y: hidden;
        font-family: 'LiberationMono', monospace;
        font-size: 10px;
        line-height: normal;
        white-space: pre-wrap;
        word-break: break-all;
        margin-bottom: 0 !important;
    }

    span.print-checkbox {
        display: inline-block;
        height: 0.25cm;
        width: 0.25cm;
        margin-right: 0.1cm;
        border: 1px solid #000;
    }

    .checklist .col-5,
    .checklist .col-6 {
