    }

    .summary-items .summary-item:last-child {
        border-bottom: 5px double black;
    }

    .summary-item h2 {
        font-size: 16px;
        font-weight: bold;
        text-transform: uppercase;
    }

    .summary-item h3 {
        margin-top: 0.4cm;
    }

    .info-block h3 {
        position: relative;
        display: inline-block;
        text-transform: uppercase;
        border-bottom: 2px solid black;
        font-size: 12px;
        line-height: 13px;
        font-weight: 900;
        margin-bottom: 0.2cm;
        padding-bottom: 0.1cm;
    }

