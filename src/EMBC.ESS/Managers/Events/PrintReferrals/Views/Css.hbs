        position: relative;
        height: 2.9cm;
        position: relative;
    }

    .summary-block {
        position: absolute;
        height: 2.7cm;
        z-index: 3;
        margin-left: -4px;
        margin-top: -1px;
    }

    .summary-titles {
        position: absolute;
        left: 11cm;
        float: right;
        text-align: right;
        padding-top: 0.9cm;
        padding-right: 1.3cm;
    }

        .summary-titles div {
            font-size: 12px;
            font-weight: bold;
            margin-bottom: 0.2cm;
        }

        .summary-titles h2 {
            font-size: 20px;
            font-weight: bold;
            text-transform: uppercase;
            margin-bottom: 0.2cm;
        }

        .summary-titles h3 {
            font-size: 13px;
            font-weight: bold;
            text-transform: uppercase;
            color: #ccc;
            margin-bottom: 0.2cm;
        }

    .summary-block h1 {
        font-size: 20px;
        font-weight: bold;
        margin-top: 2cm;
        margin-left: 0.65cm;
        width: 20cm;
    }

    .stamp-goes-here {
        height: 4cm;
        padding-top: 2cm;
    }

        .stamp-goes-here p {
            color: rgb(204, 204, 204, 0.5);
            text-transform: uppercase;
            text-align: center;
            font-size: 17px;
            font-weight: normal;
        }

    .info-block {
        margin: 0.1cm;
        padding: 0;
        padding-left: 0.5cm;
    }

        .info-block.first {
            margin-top: 0;
        }

    .bigger * {
        font-size: 12px !important;
    }

    .outlined {
        border: 3px double black;
        margin: 0cm;
        padding: 0.2cm;
        padding-left: 0.7cm;
        height: 100%;
    }

    .summary-item {
        border-top: 5px double black;
