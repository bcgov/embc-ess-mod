    .watermark {
        transform: rotate(-52deg);
        opacity: 0.5;
        color: #ccc;
        font-size: 11.2em;
        font-weight: bold;
        z-index: -5000;
        position: absolute;
        top: 8cm;
        left: -1.5cm;
        text-transform: uppercase;
        word-wrap: break-word;
        text-align: center;
