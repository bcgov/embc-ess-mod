        padding: 0;
        font-size: 11px;
        box-sizing: border-box;
    }

    .header,
    .header-space {
        height: 3.4cm;
    }

    .header {
        top: 0;
        padding: 0.75cm;
    }

    .content {
        width: 21.6cm;
        padding: 0 0.75cm;
        background-image: url('data:image/svg+xml;base64,');
        background-repeat: repeat-y;
    }

    body {
        margin: 0;
        padding: 0;
        font-size: 14px;
        background-color: #fff;
    }

    strong {
        font-weight: bold;
    }

    em {
        font-style: italic;
    }

        em strong {
            font-style: italic;
            font-weight: bold;
        }

