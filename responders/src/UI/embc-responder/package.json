{
  "name": "embc-responder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run gen-api && ng serve --port 6200 --proxy-config proxy.dev.conf.json",
    "startlocal": "npm run gen-api-local && ng serve --port 6200 --proxy-config proxy.conf.json",
    "restart": "npm run gen-api",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen-api": "ng-openapi-gen -i https://era-responders-dev.apps.silver.devops.gov.bc.ca/api/openapi/v1/openapi.json -o ./src/app/core/api --ignoreUnusedModels false",
    "gen-api-local": "ng-openapi-gen -i http://localhost:6020/api/openapi/v1/openapi.json -o ./src/app/core/api --ignoreUnusedModels false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.3",
    "@angular/cdk": "^14.1.3",
    "@angular/common": "~14.2.3",
    "@angular/compiler": "~14.2.3",
    "@angular/core": "~14.2.3",
    "@angular/forms": "~14.2.3",
    "@angular/material": "^14.1.3",
    "@angular/platform-browser": "~14.2.3",
    "@angular/platform-browser-dynamic": "~14.2.3",
    "@angular/router": "~14.2.3",
    "@ng-idle/core": "~11.1.0",
    "@ng-idle/keepalive": "~11.0.3",
    "angular-oauth2-oidc": "^10.0.3",
    "angular2-text-mask": "~9.0.0",
    "bootstrap": "~5.1.3",
    "eslint-plugin-deprecation": "~1.3.2",
    "moment": "^2.29.4",
    "ngx-markdown": "^14.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.3",
    "@angular-eslint/builder": "^14.0.3",
    "@angular-eslint/eslint-plugin": "^14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.2",
    "@angular-eslint/schematics": "~14.0.2",
    "@angular-eslint/template-parser": "~14.0.2",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "~14.2.3",
    "@types/jasmine": "^3.10.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.5",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.4.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-openapi-gen": "^0.21.2",
    "prettier": "^2.6.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.4"
  }
}