{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft ": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System.Net.Http.HttpClient": "Warning",
        "Grpc.Net.Client": "Warning"
      }
    }
  },
  "messaging": {
    "mode": "client"
  },
  "AllowedHosts": "*"
}